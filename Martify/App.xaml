<Application x:Class="Martify.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Martify"
             StartupUri="LoginWindow.xaml">
    
    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="/Resources/XAML/MaterialDesignRes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/XAML/TriggerRes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/XAML/FontRes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/XAML/MainResource.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/XAML/ImageRes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/XAML/ViewRes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/XAML/StyleRes.xaml"></ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
    