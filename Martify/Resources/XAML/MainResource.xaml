<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:ViewModels="clr-namespace:Martify.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ViewModels:MainVM x:Key="MainVM"></ViewModels:MainVM>
    <ViewModels:LoginVM x:Key="LoginVM"></ViewModels:LoginVM>
    <ViewModels:DashboardVM x:Key="DashboardVM"></ViewModels:DashboardVM>
    <ViewModels:ProductsVM x:Key="ProductsVM"></ViewModels:ProductsVM>
    <ViewModels:EmployeeVM x:Key="EmployeeVM"></ViewModels:EmployeeVM>

    <Storyboard x:Key="GradientRotateStoryboard"
                    RepeatBehavior="Forever">
        <DoubleAnimation Storyboard.TargetName="AnimatedBorderBrush"
                             Storyboard.TargetProperty="Transform.Children[0].Angle"
                             From="0" To="360" Duration="0:0:5" />
    </Storyboard>
    
</ResourceDictionary>